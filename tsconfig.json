{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es6"
    ],
    "noEmitOnError": true,
    "allowUnreachableCode": true
  },
  "include": [
    "src",
    "test"
  ],
  "parcelTsPluginOptions": {
    "transpileOnly": false
  }
}
